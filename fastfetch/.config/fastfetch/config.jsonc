{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "padding": {
            "top": 2
        }
    },
    "display": {
        "separator": "",
        "key": {
            "width": 15
        }
    },
    "modules": [
        {
            "type": "custom",
            "format": "psst... you look nice today",
            "outputColor": "light_black"
        },
        {
            "type": "custom",
            "format": "{#32}------{#33}------{#31}------{#35}------{#34}------{#36}------"
        },
        {
            "key": "User",
            "type": "title",
            "format": "{user-name}",
            "keyColor": "green"
        },
        {
            "key": "Hostname",
            "type": "title",
            "format": "{host-name}",
            "keyColor": "green"
        },
        {
            "key": "OS",
            "type": "os",
            "format": "{pretty-name} {?version-id} {?arch}",
            "keyColor": "yellow"
        },
        {
            "key": "Model",
            "type": "host",
            "keyColor": "yellow"
        },
        {
            "key": "Kernel",
            "type": "kernel",
            "keyColor": "yellow"
        },
        {
            "key": "Uptime",
            "type": "uptime",
            "keyColor": "red"
        },
        {
            "key": "Load Average",
            "type": "loadavg",
            "keyColor": "red"
        },
        {
            "key": "Processes",
            "type": "processes",
            "keyColor": "red"
        },
        {
            "key": "WM",
            "type": "wm",
            "keyColor": "magenta"
        },
        {
            "key": "DE",
            "type": "de",
            "keyColor": "magenta"
        },
        {
            "key": "Terminal",
            "type": "terminal",
            "format": "{pretty-name} {version}",
            "keyColor": "magenta"
        },
        {
            "key": "Shell",
            "type": "shell",
            "keyColor": "magenta"
        },
        {
            "key": "Editor",
            "type": "editor",
            "keyColor": "magenta"
        },
        {
            "key": "Packages",
            "type": "packages",
            "format": "{all}",
            "keyColor": "magenta"
        },
        {
            "key": "CPU",
            "type": "cpu",
            "format": "{cores-logical} x {name}",
            "keyColor": "blue"
        },
        {
            "key": "GPU",
            "type": "gpu",
            "format": "{name}",
            "keyColor": "blue"
        },
        {
            "key": "RAM",
            "type": "memory",
            "keyColor": "blue"
        },
        {
            "key": "Disk",
            "type": "disk",
            "folders": "/",
            "keyColor": "blue"
        },
        {
            "key": "Battery",
            "type": "battery",
            "keyColor": "blue"
        },
        {
            "key": "LAN IPv4",
            "type": "localip",
            "showPrefixLen": false,
            "keyColor": "cyan"
        },
        {
            "key": "LAN IPv6",
            "type": "localip",
            "showIpv6": true,
            "showPrefixLen": false,
            "keyColor": "cyan",
            "format": "{2}"
        },
        {
            "key": "WAN IPv4",
            "type": "publicip",
            "timeout": 1000,
            "keyColor": "cyan"
        },
        {
            "type": "custom",
            "format": "{#36}------{#34}------{#35}------{#31}------{#33}------{#32}------"
        },
        "break"
    ]
}
